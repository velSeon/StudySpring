<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id = "firstStudent" class="com.dto.Student">
<constructor-arg name = "name" value="홍길동"/>  <!-- name의 "x"가 무엇? Student 생성자의 변수 x , 생성자 변수 name으로 바꿀 시 name으로 바뀜. 변수명임.-->
</bean>

<bean id = "secondStudent" class="com.dto.Student">
<constructor-arg name = "age" value= "20"></constructor-arg>
</bean>

<bean id ="thirdStudent" class="com.dto.Student">
<constructor-arg name ="name" value="유관순"></constructor-arg>
<constructor-arg name ="age" value="20"></constructor-arg>
</bean>
<!-- <class="com.dto.Student"></bean>Student에는 기본 생성자가 없기때문에 에러 발생 -->

</beans>
